{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": ["./src/app/*"],
      "@components": ["./src/components/index"],
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@assets/*": ["./src/assets/*"],
      "@services/*": ["./src/services/*"],
      "@styles/*": ["./src/styles/*"],
      "@store": ["./src/store/index"],
      "@store/*": ["./src/store/*"]
    }
  }
}
